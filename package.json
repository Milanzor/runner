{
  "name": "@milanzor/runner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "cross-env NODE_ENV=production node ./bin/www",
    "example": "cross-env yarn start --config ./docs/demo-dirs/example.runners.json",
    "nodemon": "nodemon -i runners.json -i ./docs/demo-dirs/example.runners.json --exec yarn example",
    "travis": "yarn build && yarn example --travistest"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vuetify": "^1.0.19",
    "ansi-to-html": "^0.6.4",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-ws-routes": "^1.1.0",
    "helmet": "^3.12.1",
    "http-errors": "~1.6.2",
    "linerstream": "^0.2.0",
    "material-design-icons-iconfont-only": "^3.0.1",
    "minimist": "^1.2.0",
    "morgan": "~1.9.0",
    "socket.io": "^2.1.1",
    "tree-kill": "^1.2.0",
    "vue-socket.io": "^2.1.1-b"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0-beta.49",
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.5",
    "@vue/cli-service": "^3.0.0-rc.5",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "vue-cli-plugin-vuetify": "^0.1.6",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
